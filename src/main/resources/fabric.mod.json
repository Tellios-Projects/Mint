{
  "schemaVersion": 1,
  "id": "mint",
  "version": "${version}",

  "name": "El & L's Dye Mod",
  "description": "Adds 20 unique dyes, alongside decorative blocks, crops, and gameplay features",
  "authors": [
    "Leah (@leafenzio on Twitter)",
    "Eliza (@tellioaridoitsu)",
    "Hecco (@Hecco)"
  ],
  "contributors": [
    "Eka (Eko-byte)",
    "Enri (@enriarts_)",
    "Zucchini (@zucchinibyday)"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/el-and-ls-dye-mod",
    "sources": "https://github.com/Tellios-Projects/Mint",
    "issues": "https://github.com/Tellios-Projects/Mint/issues"
  },

  "license": "MIT",
  "icon": "assets/mint/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "net.leafenzo.mint.ModInit"
    }],
    "client": [{
      "adapter": "kotlin",
      "value":  "net.leafenzo.mint.ModClientInit"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value":  "net.leafenzo.mint.datageneration.ModDataGenerator"
    }]
  },
  "accessWidener": "mint.accesswidener",
  "mixins": [
    "mint.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.10.8+kotlin.1.9.0"
  },
  "suggests": {
    "another-mod": "*"
  },
  "conflicts": {

  },
  "breaks": {
    "dye-depot": "*"
  }
}
