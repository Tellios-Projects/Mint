{
  "schemaVersion": 1,
  "id": "mint",
  "version": "${version}",

  "name": "El & L's Dyes",
  "description": "Mint and Mint Colored Dye for Minecraft Java Edition!",
  "authors": [
    "Leah (@leafenzio), Eliza (@tellioaridoitsu), and Eka (Eko-byte)"
  ],
  "contact": {
    "homepage": "TODO ADDME - website link",
    "sources": "https://github.com/Tellios-Projects/Mint"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "net.leafenzo.mint.ModInit"
    }],
    "client": [{
      "adapter": "kotlin",
      "value":  "net.leafenzo.mint.ModClientInit"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value":  "net.leafenzo.mint.datageneration.ModDataGenerator"
    }],
    "mm:early_risers": [{
      "adapter": "kotlin",
      "value": "net.leafenzo.mint.ModEarlyRiser"
    }]
  },
  "mixins": [
    "mint.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.10.8+kotlin.1.9.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}
